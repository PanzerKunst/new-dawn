{
  "name": "New Dawn Wordpress theme",
  "description": "",
  "main": "",
  "authors": [
    "Christophe Bram"
  ],
  "license": "GPL V3",
  "homepage": "https://github.com/PanzerKunst/new-dawn/wordpress/web/app/themes/new-dawn",
  "moduleType": [],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "bootstrap-sass": "~3.3.6",
    "bourbon": "~4.2.6",
    "fastclick": "~1.0.6",
    "font-awesome": "~4.5.0",
    "gsap": "~1.18.2",
    "lodash": "~3.10.1",
    "pjs": "jneen/pjs#~5.1.1",
    "js-breakpoints": "~1.0.2",
    "jquery-mousewheel": "~3.1.13"
  },
  "overrides": {
    "jquery": {
      "main": [
        "./dist/jquery.min.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "./assets/stylesheets/_bootstrap.scss",
        "./assets/javascripts/bootstrap.min.js"
      ]
    },
    "font-awesome": {
      "main": [
        "./scss/font-awesome.scss",
        "./fonts/*"
      ]
    },
    "gsap": {
      "main": [
        "./src/minified/TweenLite.min.js",
        "./src/minified/easing/EasePack.min.js",
        "./src/minified/plugins/CSSPlugin.min.js",
        "./src/minified/plugins/ScrollToPlugin.min.js"
      ]
    },
    "lodash": {
      "main": [
        "./lodash.min.js"
      ]
    },
    "pjs": {
      "main": [
        "./src/p.js"
      ]
    },
    "js-breakpoints": {
      "main": [
        "./breakpoints.js",
        "./breakpoints.scss"
      ]
    },
    "jquery-mousewheel": {
      "main": [
        "./jquery.mousewheel.min.js"
      ]
    }
  }
}
